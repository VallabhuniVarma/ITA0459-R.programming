# Create two vectors for input
vector1 <- c(1, 2, 3, 4, 5, 6, 7, 8, 9)
vector2 <- c(10, 11, 12, 13, 14, 15, 16, 17, 18)

# Combine the vectors into a matrix with 3 columns
matrix_input <- cbind(vector1, vector2)

# Create a 3x3x2 array using the matrix
my_array <- array(matrix_input, dim = c(3, 3, 2))

# Print the array
print(my_array)
