# Display the aim of the program
cat("This program prints numbers from 1 to 100 and prints 'Fizz' for multiples of 3, 'Buzz' for multiples of 5, and 'FizzBuzz' for multiples of both.\n\n")

# Loop through numbers from 1 to 10
for (i in 1:10) {
  output <- ""
  
  if (i %% 3 == 0) {
    output <- paste(output, "Fizz", sep = "")
  }
  
  if (i %% 5 == 0) {
    output <- paste(output, "Buzz", sep = "")
  }
  
  if (output == "") {
    output <- i
  }
  
  cat(output, "\n")
}

