# Empty plot without specified axes limits
plot(1, type = "n", main = "Empty Plot without Axes Limits", xlab = "X-axis", ylab = "Y-axis")

# Empty plot with specified axes limits
plot(1, type = "n", main = "Empty Plot with Axes Limits", xlab = "X-axis", ylab = "Y-axis", xlim = c(0, 10), ylim = c(0, 20))
